{
  "expo": {
    "name": "Router Sandbox",
    "slug": "expo-router-sandbox",
    "scheme": "sandbox",
    "splash": {
      "image": "./splash.png",
      "backgroundColor": "#1c2026"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "router": {
        "redirects": [
          {
            "from": "/about",
            "to": "/about-us"
          },
          {
            "from": "/source",
            "to": "https://github.com/"
          }
        ]
      }
    }
  }
}
