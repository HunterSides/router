{
    "title": "expo-router config plugin options",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/Props",
    "definitions": {
        "Redirect": {
            "description": "Represents criteria for matching a request and the redirect destination.",
            "properties": {
                "from": {
                    "description": "Glob pattern to match against. Uses `path-to-regexp` to perform matching.",
                    "type": "string"
                },
                "to": {
                    "description": "String to redirect to given the criteria match the incoming request.",
                    "type": "string"
                },
                "has": {
                    "description": "Criteria for attributes that must be present in order to match the request.",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/RedirectMatchable"
                    }
                },
                "exclude": {
                    "description": "Criteria for attributes that must be present in order to match the request.",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/RedirectMatchable"
                    }
                }
            },
            "required": ["from", "to"],
            "additionalProperties": false
        },
        "RedirectMatchable": {
            "description": "Criteria for matching an attribute of a request.",
            "properties": {
                "type": {
                    "description": "The request object to match against. Only `query` is supported in client-side redirects.",
                    "type": "string",
                    "enum": [
                        "host",
                        "header",
                        "cookie",
                        "query"
                    ]
                },
                "key": {
                    "description": "Key inside the request object for `type`.",
                    "type": "string"
                },
                "value": {
                    "description": "If provided, the value for `key` in the incoming request object must match, i.e. `request[type][key] === value`.",
                    "type": "string"
                }
            },
            "required": ["type", "key"],
            "additionalProperties": false
        },
        "AsyncRouteOption": {
            "description": "Should Async Routes be enabled, currently only `development` is supported.",
            "oneOf": [
                {
                    "type": "string",
                    "enum": [
                        "development",
                        "production"
                    ]
                },
                {
                    "type": "boolean"
                }
            ]
        },
        "Props": {
            "type": "object",
            "properties": {
                "origin": {
                    "description": "Production origin URL where assets in the public folder are hosted. The fetch function is polyfilled to support relative requests from this origin in production, development origin is inferred using the Expo CLI development server.",
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                },
                "headOrigin": {
                    "description": "A more specific origin URL used in the `expo-router/head` module for iOS handoff. Defaults to `origin`.",
                    "type": "string"
                },
                "unstable_src": {
                    "description": "Changes the routes directory from `app` to another value. Defaults to `app`. Avoid using this property.",
                    "type": "string"
                },
                "asyncRoutes": {
                    "description": "Should Async Routes be enabled, currently only `development` is supported.",
                    "oneOf": [
                        {
                            "$ref": "#/definitions/AsyncRouteOption"
                        },
                        {
                            "type": "object",
                            "properties": {
                                "ios": {
                                    "$ref": "#/definitions/AsyncRouteOption"
                                },
                                "android": {
                                    "$ref": "#/definitions/AsyncRouteOption"
                                },
                                "web": {
                                    "$ref": "#/definitions/AsyncRouteOption"
                                },
                                "default": {
                                    "$ref": "#/definitions/AsyncRouteOption"
                                }
                            },
                            "additionalProperties": true
                        }
                    ]
                },
                "redirects": {
                    "description": "List of objects which match a path and redirect elsewhere",
                    "type": "array",
                    "items":{
                        "$ref": "/definitions/Redirect"
                    }
                }
            },
            "required": ["origin"],
            "additionalProperties": false
        }
    }
}